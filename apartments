<script>
        $(document).ready(function(){
            var visited = $.cookie("pagesVisited");
            var uauth = parseInt($.cookie("uauth"), 10);
            if (isNaN(uauth)){
                uauth = 0;
            }
            if (visited == null){
                visited = 0;
            }
            //visited++;
            ///$.cookie("pagesVisited", visited, { expires: 60, path: '/' });

            if ( ((($.cookie('noPopup') == 0) || ($.cookie('noPopup') == null)) && (($.cookie('pagesVisited')== 10) || ($.cookie('pagesVisited')== 20)) && uauth==0 ) ){
                ModalBox.open("#reminderDialog");
            }else{
                $("#reminderDialog").css("display", "none");
            }

            $('#closePopup').click(
                    function() {
                        $("#reminderDialog").css("display", "none");
                        if($.cookie('pagesVisited')== 20){
                            $.cookie("noPopup", "1", { expires: 59, path: '/' });
                        }
                    }
            );
            $('#closeLoginForm').click(
                    function() {
                        $("#signIn").css("display", "none");
                        $('#lightbox-overlay').each(function() {
                            $(this).css({ opacity: 0 });
                        });
                    }
            );
        });
    </script>
